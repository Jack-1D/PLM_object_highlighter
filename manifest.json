{
    "manifest_version": 3,
    "name": "PLM_object_highlighter",
    "description": "PLM crawler",
    "version": "1.0",
    "action": {
        "default_popup": "templates/popup.html",
        "default_icon": {
            "16": "/images/icon_16.png",
            "32": "/images/icon_32.png",
            "48": "/images/icon_48.png",
            "128": "/images/icon_128.png"
        }
    },
    "icons": {
        "16": "/images/icon_16.png",
        "32": "/images/icon_32.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "debugger",
        "windows",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}